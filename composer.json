{
	"name": "apermo/wordpress-sovereignty",
	"description": "Pure Zen",
	"require": {
		"php": ">=7.4.0",
		"composer/installers": "~2.2"
	},
	"type": "wordpress-theme",
	"license": "MIT",
	"authors": [
		{
			"name": "Matthias Pfefferle",
			"homepage": "https://notiz.blog"
		},
		{
			"name": "Christoph Daum",
			"homepage": "https://christoph-daum.com"
		}
	],
	"extra": {
		"installer-name": "sovereignty"
	},
	"require-dev": {
		"roave/security-advisories": "dev-latest",
		"johnpbloch/wordpress": "6.7.2",
		"squizlabs/php_codesniffer": "^3.11",
		"wp-coding-standards/wpcs": "^3.1",
		"slevomat/coding-standard": "^8.15"
	},
	"config": {
		"allow-plugins": {
			"composer/installers": true,
			"dealerdirect/phpcodesniffer-composer-installer": true,
			"johnpbloch/wordpress-core-installer": false
		}
	}
}
